      Constants:
      -String.digits                  -String.asciiLetters  
      -String.asciiLettersLowerCase   -String.asciiLettersUpperCase 
      -String.hexDigits               -String.octDigits
               "
      return 0
   fi
   while (($#))
   do
      local match=$(Boolean.FALSE)  
      case $1 in 
         base64Decode|all)
            match=$(Boolean.TRUE)
            System.${printStream}.println "
               base64Decode
               Parameters:
                  None 
               Returns:
                  base64 decoded data
               Throws:
                  NotInPathException - if base64 not in path
                  DecoderException - if problem with decoding
               Example:
                  String a YWJjCg==
                  a.base64Decode
                  >>>abc  
            ";;&
         base64Encode|all)
            match=$(Boolean.TRUE)
            System.${printStream}.println "
               base64Encode
               Parameters:
                  None
               Returns:
                  base64 encoded data
               Throws:
                  NotInPathException - if base64 not in path
               Example:
                  String a abc
                  a.base64Encode
                  >>>YWJjCg==
            ";;&
         capitalize|all)
            match=$(Boolean.TRUE)
