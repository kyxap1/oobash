# zfill
# Parameters:
#    an integer  - The width of the return String
# Returns:
#    Pad a numeric string on the left with zero digits until the given width is reached. 
# Example:
#    String a abc
#    a.zfill 5
#    >>>00abc
__zfill()
{
   if (( $# == 3 ))
   then
      local this="$1"
      (declare -i width=$3) 2>/dev/null ||  (return 1)
      if (( $? != 0 ))
      then
         __throw "IllegalArgumentException" ": Wrong argument: $3"
         return 1         
      fi
      declare -i width=$3
      length=$("$this".length)
      if (( $length < $width ))
      then
         local left=$(($width-$("$this".length)))
         printf -v pad "%."$left"d"
         System.out.println "$pad""$("$this".toString)"
      else
         System.out.println "$("$this".toString)"
      fi
      return 0
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

# swapCase
# Parameters:
#    None
# Returns:
#    a copy of s, but with lower case letters converted to upper case and vice versa. 
# Example:
#    String a AbC
#    a.swapCase
#    >>>aBc    
__swapCase()
{
   if (( $# == 2 ))
   then
      local this="$1"
      local str="$("$this".toString)"
      local swaped=''
      declare -i i=0
      for ((i=0; i < ${#str}; i++))
      do
         local char="${str:i:1}"
         case $char in
            # small sharp s
            \DF )                  
            swaped="$swaped""SS";;
            ###############        
            # ignore case #
            ###############
            * ) 
            swaped="$swaped""${char~}" ;;
         esac
      done
      System.out.println "$swaped"
      return 0   
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

# center  
#    This function centers a string in a field of given width. It returns a string that is at least width characters wide, 
#    created by padding the string with the space character until the given width on the right, left or both sides.
# Parameters:
#    width - integer
# Returns:
#    centered string
# Example:
#    String a abc
#    a.center 5
#    >>>abc 
#    echo "'$(a.center 5)'"
#    >>>' abc '
__center()
{
   if (( $# == 3 ))
   then
      local this="$1"
      (declare -i width=$3) 2>/dev/null ||  (return 1)
      if (( $? != 0 ))
      then
         __throw "IllegalArgumentException" ": Wrong argument: $3"
         return 1         
      fi
      declare -i width=$3
      if (( $("$this".length) < $width ))
      then
         declare -i spaces=$width-$("$this".length)
         declare -i lspaces=$spaces/2
         declare -i rspaces=$spaces-$lspaces
         declare -i lruns=0
         declare -i rruns=0
         declare left=''
         declare right=''
         while (( $lruns < $lspaces ))
         do
            left+=' '
            lruns+=1
         done
         while (( $rruns < $rspaces ))
         do
            right+=' '
            rruns+=1
         done
         System.out.println  "$left""$("$this".toString)""$right"
      else
         System.out.println "$("$this".toString)"
      fi
      return 0
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

# equalsIgnoreCase
#    Optimized for german, french, polish, spanish, romanian, czech, slovak, slovenian, 
#    swedish, finnish, danish, scottish gaelic, catalan, hungarian, norwegian and icelandic locale.
#    Compares this String to another String, ignoring case considerations. Two strings are considered equal 
#    ignoring case if they are of the same length and corresponding characters 
#    in the two strings are equal ignoring case.
#    Two characters c1 and c2 are considered the same ignoring case if at least one of the following is true:
#       * The two characters are the same (as compared by the == operator)
#       * Applying the method Character.toUpperCase(char) to each character produces the same result
#       * Applying the method Character.toLowerCase(char) to each character produces the same result 
# Parameters:
#    a String object - The String object to compare this String against
# Returns:
#    true if the argument is not null and it represents an equivalent String ignoring case; 
#    false otherwise
# Example:
#    String a abc
#    String b AbC
#    String c ddd
#    File d abc
#    a.equalsIgnoreCase b
#    >>>true
#    a.equalsIgnoreCase c
#    >>>false 
#    a.equalsIgnoreCase d
#    >>>WrongClassException d: class File
__equalsIgnoreCase()
{
   if (( $# == 3 ))
   then
      local this="$1"
      local argobject="$3"
      local argclass
      argclass="$("$argobject".getClass 2>/dev/null)" 
      if (( $? == 0 ))
      then
         if [[ "$("$this".getClass)" == "$argclass" ]]
         then
            if [[ "$("$argobject".toUpperCase)" == "$("$this".toUpperCase)" ]]
            then
               Boolean.TRUE
               return 0
            else
               Boolean.FALSE
               return 0
            fi
         fi
         __throw "WrongClassException" "argument $argobject: $("$argobject".getClass)"
         return 1
      fi
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

