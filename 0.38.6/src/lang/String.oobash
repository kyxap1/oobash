# compareToIgnoreCase
#    Compares two strings lexicographically, ignoring case differences. 
#    This method returns an integer whose sign is that of calling 
#    compareTo with normalized versions of the strings where case differences 
#    have been eliminated.
# Parameters:
#    str - the String to be compared.
# Returns:
#    a negative integer, zero, or a positive integer as the specified String 
#    is greater than, equal to, or less than this String, 
#    ignoring case considerations.
# Example:
#    String a ab
#    x.compareToIgnoreCase abc
#    >>>-1
#    x.compareToIgnoreCase aY
#    >>>-23
#    x.compareToIgnoreCase ay
#    >>>-23
__compareToIgnoreCase()
{
   if (( $# == 3 ))
   then
      local this="$1"
      local str1="$("$this".toUpperCase)"
      local str2="$(__strUpper "$3")"
      declare -i length=-1
      if (( ${#str1} == ${#str2} ))
      then
         length=${#str2}
         if [[ "$str1" == "$str2" ]]
         then
            System.out.println 0
            return 0
         fi
      fi
      if (( $length == -1 ))
      then
         declare -i result=$((${#str1}-${#str2}))
         if (( ${#str1} < ${#str} ))
         then
            length=${#str1}
         else
            length=${#str2}
         fi
      fi	
      declare -i i=0
      for ((i=0; i < $length; i++))
      do    
         local charStr2="${str2:i:1}"
         declare -i valueStr2=$(printf '%d\n' "'$charStr2")
         local charStr1="${str1:i:1}"
         declare -i valueStr1=$(printf '%d\n' "'$charStr1")
         if (( $valueStr1 != $valueStr2 ))
         then
            System.out.println $(($valueStr1-$valueStr2))
            return 0
         fi		
      done
      System.out.println $result
      return 0
   fi
   local class=$2 
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1   
}

# base64Encode
#    Parameters:
#       None
#    Returns:
#       base64 encoded data
#    Example:
#       String a abc
#       a.base64Encode
#       >>>YWJjCg==
__base64Encode()
{
   if (( $# == 2 ))
   then
      local this="$1"
      if command -v base64 >/dev/null 2>&1
      then 
         System.out.println "$("$this".toString)" | base64
         return $?
      fi
      __throw "NotInPathException" ": base64 not in path (install the GNU coreutils ?)"
      return 1
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

# base64Decode
#    Parameters:
#       None 
#    Returns:
#       base64 decoded data
#    Example:
#       String a YWJjCg==
#       a.base64Decode
#       >>>abc  
__base64Decode()
{
   if (( $# == 2 ))
   then
      local this="$1"
      if command -v base64 >/dev/null 2>&1
      then       
         declare -i returnValue
         local output
         output="$(System.out.println "$("$this".toString)" | base64 -d 2>/dev/null)"
         returnValue=$?
         if (( $returnValue != 0 ))
         then
            __throw "DecoderException" ": Decoding problem with: $("$this".toString) "
         else
            System.out.println "$output"
         fi
         return $returnValue
      fi
      __throw "NotInPathException" ": base64 not in path (install the GNU coreutils ?)"
      return 1
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2} 
   return 1
}

# rstrip
# Parameters:
#    None
# Returns:
#    It returns a copy of the string in which all chars have been stripped 
#    from the end of the string (whitespace characters).
# Example:
#    String a "   abc    "
#    echo "'$(a.rstrip)'"
#    >>>'   abc'
__rstrip()
{
   if (( $# == 2 ))
   then
      local this="$1"
      local str="$("$this".toString)"
      shopt -s extglob
      # trimming...
      result="${str%%+([[:space:]])}"
      System.out.println "$result"
      return 0
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

