# startsWith
# Parameters:
#    prefix - the prefix string.
#    toffset - where to begin looking in this string. Default: 0
# Returns:
#    true if string starts with parameter string, false otherwise.
# Example:
#    String a abcdef
#    a.startsWith ab
#    >>>true
__startsWith()
{
   case $# in 
      3) declare -i toffset=0;;
      4) (declare -i tooffset=$4) 2>/dev/null ||  (return 1)
         if (( $? != 0 ))
         then
            __throw "IllegalArgumentException" ": Wrong argument: $4"
         return 1         
         fi
         declare -i toffset=$4;;
      *) local class="$2"
         local printStream=err
         ${class}.help ${FUNCNAME:2}
         return 1;;
   esac
   local this="$1"
   declare -i length=$("$this".length)
   if [[ "$("$this".substring $toffset $(($length-1)))" == "$3"* ]]
   then
      Boolean.TRUE
   else
      Boolean.FALSE
   fi
   return 0
}

# lastIndexOf
#    Returns the index within this string of the last occurrence of the specified substring, 
#    searching backward starting at the specified index. 
#    If no such value exists, then -1 is returned.
# Parameters:
#    str - the substring to search for.
#    [fromIndex - the index to start the search from.]
# Returns:
#    the index within this string of the last occurrence of the specified substring.
# Examples:
#    String a "eacc eadee"
#    a.lastIndexOf " e" 2
#    >>>-1
#    a.lastIndexOf " e" 20
#    >>>4
#    a.lastIndexOf e
#    >>>9
__lastIndexOf() 
{
   local this="$1"
   case $# in
      3) local pattern="$3"
         declare -i to=$("$this".length);;
      4) local pattern="$3"
         (declare -i to=$4 ) 2>/dev/null ||  (return 1)
         if (( $? != 0 ))
         then
            __throw "IllegalArgumentException" ": Wrong argument: $4"
            return 1         
         fi
         declare -i to=$(($4+1))
         # negative index ?
         if (( 0 > $4 ))
         then
            System.out.println -1
            return 0
         fi;;
      *) local class="$2"
         local printStream=err
         ${class}.help ${FUNCNAME:2}
         return 1;;
   esac
   local str="$("$this".toString)"
   str="${str:0:$to}"
   shopt -s extglob
   local replaced="${str//*$pattern/}"
   declare -i pattern_length=${#pattern}
   declare -i replaced_length=${#replaced}
   declare -i string_length=${#str}
   declare -i lastindex=$(($string_length-$replaced_length))
   # nothing modified --> no match --> no index
   if (( $replaced_length == $string_length ))
   then
      System.out.println -1
      return 0
   fi   
   if (( $pattern_length == 0 )) && (( $string_length < $("$this".length) ))
   then
      pattern_length=$pattern_length+1
   fi 
   # ok if match is existing and if no substring neccessary
   System.out.println $(($lastindex-$pattern_length))
   return 0
}

# indexOf
#    Returns the index within this string of the first occurrence of the specified substring, 
#    starting at the specified index. 
#    If no such value exists, then -1 is returned.
#    Simulation of following java behavior:  k >= Math.min(fromIndex, str.length()) && this.startsWith(str, k)
# Parameters:
#    str - the substring to search for.
#    [fromIndex - the index to start the search from.]
# Returns:
#    the index within this string of the first occurrence of the specified substring.
# Examples:
#    String a abc
#    a.indexOf a 
#    >>>0
__indexOf()
{
   case $# in
      3) local pattern="$3"
         declare -i from=0;;
      4) local pattern="$3"
         (declare -i from=$4) 2>/dev/null ||  (return 1)
         if (( $? != 0 ))
         then
            __throw "IllegalArgumentException" ": Wrong argument: $4"
            return 1         
         fi
         declare -i from=$4
         # negative index ?
         if (( 0 > $4 ))
         then
            from=0
         fi;;
      *) local class="$2"
         local printStream=err
         ${class}.help ${FUNCNAME:2}
         return 1;;
   esac
   local this="$1" 
   declare -i pattern_length=${#pattern}
   local str="$("$this".toString)"
   str="${str:$from}"
   declare -i strlength=${#str}
   local newstr="${str%%$pattern*}"
   declare -i newstrlength=${#newstr}
   # Java API:  
   # k >= Math.min(fromIndex, str.length()) && this.startsWith(str, k)
   # --> looking for empty string '' out of index returns sringlength instead of -1 
   if (( $pattern_length == 0 ))
   then 
      if (( $from <= $("$this".length) ))
      then
         System.out.println $from
      else
         System.out.println $("$this".length)
      fi  
      return 0 
   fi 
   if (( $strlength == $newstrlength ))
   then
      System.out.println -1
   else
      System.out.println $(($newstrlength+$from))
   fi
   return 0
}

# charAt
# Returns the char value at the specified index. An index ranges from 0 to length - 1. 
# The first char value of the sequence is at index 0, the next at index 1, and so on, as for array indexing.
# Parameters:
#    index - the index of the char value (integer)
# Returns:
#    the char value at the specified index of this string. The first char value is at index 0.
# Example:
#    String a abc
#    a.charAt 2
#    >>>c
__charAt()
{
   if (( $# == 3 ))
   then
      this="$1"
      (declare -i pos=$3) 2>/dev/null ||  (return 1)
      if (( $? != 0 ))
      then
         __throw "IllegalArgumentException" ": Wrong argument: $3"
         return 1         
      fi
      declare -i pos="$3"
      if  (( $pos <= ($("$this".length)-1) )) && (( $pos >= 0 ))
      then
         local str="$("$this".toString)"
         local char=${str:$pos:1}
         System.out.println $char
         return 0
      fi
      __throw "IndexOutOfBoundsException" " for $pos !"
      return 1
   fi
   local class="$2"
   local printStream=err
   ${class}.help ${FUNCNAME:2}
   return 1
}

